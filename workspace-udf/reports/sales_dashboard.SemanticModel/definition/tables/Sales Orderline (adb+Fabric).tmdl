table 'Sales Orderline (adb+Fabric)'
	lineageTag: de98c10b-f351-4e3a-8536-71acfa96bafa

	measure 'Net Sales(adb + Fabric)' =
			
			SUMX(
			    'Sales Orderline (adb+Fabric)',
			    ('Sales Orderline (adb+Fabric)'[LineTotal]
			    + 'Sales Orderline (adb+Fabric)'[TaxAmount]   -- optional, include if tax counts toward net sales
			))
		formatString: \$#,0.00;(\$#,0.00);\$#,0.00
		lineageTag: 5e3adce7-e9a0-4e33-9945-42fca78f8e23

	measure 'Revenue(adb + Fabric)' =
			SUMX(
			    'Sales Orderline (adb+Fabric)',
			    'Sales Orderline (adb+Fabric)'[Quantity] * 'Sales Orderline (adb+Fabric)'[UnitPrice]
			)
		formatString: \$#,0.00;(\$#,0.00);\$#,0.00
		lineageTag: 3ec9db50-5a2c-4b35-b6a6-1e61cdb49690

		annotation PBI_FormatHint = {"currencyCulture":"en-US"}

	measure NetSales(Display) =
			
			VAR TotalSales = [Net Sales(adb + Fabric)]
			VAR CurrencySymbol = "$"  -- Replace with â‚¹ or your currency
			RETURN
			SWITCH(
			    TRUE(),
			    ABS(TotalSales) >= 1000000, CurrencySymbol & FORMAT(TotalSales/1000000, "0.0") & "M",
			    ABS(TotalSales) >= 1000, CurrencySymbol & FORMAT(TotalSales/1000, "0.0") & "K",
			    CurrencySymbol & FORMAT(TotalSales, "0")
			)
		lineageTag: 0ee99bbc-5ded-4fb3-a890-6673c9d63bfc

	measure 'Net Sales Till Last Year' =
			
			CALCULATE(
			    [Net Sales(adb + Fabric)],   -- your existing Net Sales measure
			    'Sales order (adb +Fabric)'[OrderDate] <= DATE(YEAR(TODAY())-1, 12, 31)
			)
		formatString: \$#,0.00;(\$#,0.00);\$#,0.00
		lineageTag: 37948445-95fb-4d75-b135-83165e6fcfde

